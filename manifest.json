{
    "manifest_version": 3,
    "name": "Form Filler",
    "description": "A configurable extension to automatically fill forms on websites",
    "version": "1.0",
    "action": {
        "default_title": "Show alert"
    },
    "permissions": ["activeTab", "scripting"],
    "background": {
        "service_worker": "background.js"
    }
}